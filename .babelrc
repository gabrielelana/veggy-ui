{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": "add-module-exports",
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "presets": []
    },
    "test" : {
      "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": { "settings": "./src/environments/test" }
        }]
      ],
      "presets": []
    }
  }
}
